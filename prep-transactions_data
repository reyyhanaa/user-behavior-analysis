create or replace table `bigquery-sandbox-467304.user_behavior.prep_transactions` as 
select
    id,
    DATE(TIMESTAMP(date)) as date, 
    client_id,
    card_id,
    CAST(REGEXP_REPLACE(amount, r'[^0-9\.]', ' ') as NUMERIC) as amount, 
    use_chip,
    merchant_id,
    merchant_city,
    merchant_state,
    CAST(zip as string) as zip,
    mcc, 
    errors
    from `bigquery-sandbox-467304.user_behavior.transactions_data`;
